# the library's version
VERSION:=0.8.6
PACKAGE_TARNAME:=tinydtls

# tools

SHELL = /bin/sh
MKDIR = mkdir
DOXYGEN= :

top_builddir = ..
prefix = /home/firstuser/nebula/myWorkspace/leaf-data/MANGOH-YELLOW-WP77XX/mangOH-yellow-wp77xx-legato/build/wp77xx/3rdParty/tinydtls
datarootdir = ${prefix}/share
docdir = ${datarootdir}/doc/${PACKAGE_TARNAME}
htmldir = ${docdir}

DISTDIR?=$(top_builddir)/tinydtls-0.8.6
FILES:=Makefile.in Doxyfile.in html

doc:	Doxyfile
	$(DOXYGEN) $< >./doxygen.out 2>&1 

clean:
	@rm -rf html

distclean:	clean
	@rm -rf $(DISTDIR)
	@rm -f *~ 

dist:	doc
	test -d $(DISTDIR)/doc || mkdir $(DISTDIR)/doc
	cp -r $(FILES) $(DISTDIR)/doc

install:	$(doc)
	if test -e html ; then mkdir -p $(htmldir); cp -r html/* $(htmldir) ; fi
